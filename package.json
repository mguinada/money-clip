{
  "name": "money-clip",
  "dependencies": {
    "@js-joda/core": "3.2.0",
    "@js-joda/locale_en-us": "3.1.1",
    "@js-joda/timezone": "2.5.0",
    "highlight.js": "11.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@open-wc/testing": "^3.0.0-next.5",
    "@open-wc/testing-karma": "^4.0.9",
    "chai": "^4.3.8",
    "deepmerge": "^4.3.1",
    "karma": "^6.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-cli": "^2.0.0",
    "karma-cljs-test": "0.1.0",
    "karma-junit-reporter": "2.0.1",
    "karma-mocha": "^2.0.1",
    "mocha": "^10.2.0",
    "shadow-cljs": "2.23.3"
  },
  "scripts": {
    "ancient": "clojure -Sdeps '{:deps {com.github.liquidz/antq {:mvn/version \"RELEASE\"}}}' -m antq.core",
    "watch": "npx shadow-cljs watch app browser-test karma-test",
    "test": "./node_modules/karma/bin/karma start --coverage",
    "test:watch": "./node_modules/karma/bin/karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "./node_modules/karma/bin/karma start --update-snapshots",
    "test:prune-snapshots": "./node_modules/karma/bin/karma start --prune-snapshots",
    "release": "npx shadow-cljs release app",
    "build-report": "npx shadow-cljs run shadow.cljs.build-report app target/build-report.html",
    "ci": "npx shadow-cljs compile karma-test && npx karma start --single-run --reporters junit,dots"
  }
}
